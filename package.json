{
  "name": "greenlight",
  "version": "0.1.0",
  "description": "Progressive Web App that aims to make tracking of deployments easy",
  "main": "./server/index.js",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint --config ./.eslintrc.js ./server/*",
    "start": "node ./server/index.js",
    "dev:run": "nodemon -L",
    "dev:build": "./node_modules/.bin/webpack --config ./config/webpack.dev.js",
    "dev:server": "./node_modules/.bin/webpack-dev-server --config ./config/webpack.dev.js --port 9191",
    "build": "./node_modules/.bin/webpack --config ./config/webpack.prod.js",
    "deploy:heroku": "./scripts/heroku.sh",
    "preinstall": "chmod +x ./scripts/preinstall.sh && ./scripts/preinstall.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EdgeJay/greenlight.git"
  },
  "keywords": [
    "pwa",
    "vue",
    "js"
  ],
  "author": "Wu Huijie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EdgeJay/greenlight/issues"
  },
  "homepage": "https://github.com/EdgeJay/greenlight#readme",
  "engines": {
    "node": "7.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.3.3",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "vue-loader": "^11.3.4",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "dustjs-linkedin": "^2.7.5",
    "koa": "^2.2.0",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-views": "^6.0.1",
    "vue": "^2.2.6",
    "vue-router": "^2.4.0",
    "vuex": "^2.2.1"
  }
}
